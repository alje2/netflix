{
  "name": "netflix-clone",
  "version": "0.0.1",
  "description": "A Netflix clone with Quasar Framework",
  "productName": "Netflix clone",
  "author": "Aleksa Radosavljevic <radosavljevic.aleksa2@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "concurrently \"quasar dev\" \"nodemon server.js\"",
    "build": "npx quasar build",
    "start": "node server.js",
    "heroku-postbuild": "yarn && yarn build",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.1",
    "axios": "^0.27.2",
    "json-server": "^0.17.0",
    "pinia": "^2.0.20",
    "quasar": "^2.7.7",
    "vue": "^3.0.0",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.5.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.5.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
